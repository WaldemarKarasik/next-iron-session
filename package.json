{
  "name": "with-iron-session",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "16.3.0"
      }
    },
    "plugins": [
      "react",
      "react-hooks"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react/no-unescaped-entities": "off",
      "react/prop-types": "warn"
    },
    "ignorePatterns": [
      "node_modules/"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "next": "9.5.4",
    "next-iron-session": "4.1.7",
    "node-fetch": "2.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "swr": "0.3.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "antd": "^4.5.2",
    "axios": "^0.19.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.27",
    "next-redux-wrapper": "^6.0.2",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.10"
  }
}
